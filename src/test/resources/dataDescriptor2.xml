<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Get all compositions of Johann Sebastian Bach -->
<dataDescriptor>
	<workflow>
		<request>
			<url>https://en.wikipedia.org/wiki/List_of_compositions_by_Johann_Sebastian_Bach</url>
		</request>
		<saveAll path="//div[@id='mw-content-text']/table[@class='infobox vcard']/tbody/tr/th[contains(., 'Founders')]/following-sibling::td/div/ul/li/a" var="founders" />
		<saveMap var="compositionNames">
			<key path="//table[@class='wikitable sortable']/tbody/tr[not(td[@colspan='8'])]/td[1]"/>
			<value path="//table[@class='wikitable sortable']/tbody/tr[not(td[@colspan='8'])]/td[4]"/>
		</saveMap>
		<saveMap var="compositionDates">
			<key path="//table[@class='wikitable sortable']/tbody/tr[not(td[@colspan='8'])]/td[1]"/>
			<value path="//table[@class='wikitable sortable']/tbody/tr[not(td[@colspan='8'])]/td[3]"/>
		</saveMap>
	</workflow>
</dataDescriptor>